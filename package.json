{
  "name": "eslint-plugin-anti-trojan-source",
  "version": "0.0.0-development",
  "description": "ESLint plugin to detect and stop Trojan Source attacks",
  "main": "./dist/index.js",
  "files": [
    "dist/**",
    "src"
  ],
  "scripts": {
    "build": "tsup src/* --no-splitting --minify",
    "test": "npm run build && mocha"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "plugin",
    "eslint plugin",
    "rules",
    "trojan",
    "trojan source",
    "source",
    "unicode",
    "bidi",
    "bidirectional attacks",
    "bidi attacks",
    "bidi security",
    "unicode security",
    "attacks",
    "security"
  ],
  "dependencies": {
    "anti-trojan-source": "^1.3.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.2",
    "@types/estree": "^0.0.50",
    "@types/node": "^16.11.6",
    "eslint": "^8.2.0",
    "mocha": "^9.1.3",
    "tsup": "^5.6.0",
    "typescript": "^4.4.4"
  },
  "homepage": "https://github.com/lirantal/eslint-plugin-anti-trojan-source",
  "bugs": {
    "url": "https://github.com/lirantal/eslint-plugin-anti-trojan-source/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lirantal/eslint-plugin-anti-trojan-source.git"
  },
  "author": {
    "name": "Liran Tal",
    "email": "liran.tal@gmail.com",
    "url": "https://github.com/lirantal"
  },
  "license": "Apache-2.0",
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        }
      ]
    }
  }
}
